{
    "name": "mktcode/contao-pwa-bundle",
    "type": "contao-bundle",
    "description": "Contao 4 bundle to turn your website into a progressive web app",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Markus Kottl√§nder",
            "homepage": "https://markus-kottlaender.de"
        }
    ],
    "require": {
        "php": "^7.1",
        "contao/core-bundle": "dev-master",
        "symfony/framework-bundle": "^4.2"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3.1",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.6",
        "leofeyer/optimize-native-functions-fixer": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message-factory": "^1.0.2",
        "phpunit/phpunit": "^7.0",
        "symfony/phpunit-bridge": "^3.4.5"
    },
    "extra": {
        "contao-manager-plugin": "Mktcode\\ContaoPwaBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Mktcode\\ContaoPwaBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mktcode\\ContaoPwaBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/mktcode/contao-pwa-bundle/issues",
        "source": "https://github.com/mktcode/contao-pwa-bundle"
    }
}
